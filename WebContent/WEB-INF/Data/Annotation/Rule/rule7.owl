<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE rdf:RDF [
  <!ENTITY swrl  "http://www.w3.org/2003/11/swrl#" >
  <!ENTITY rule7 "http://www.hut.edu.vn/ontology/rule7#" >
  <!ENTITY owl "http://www.w3.org/2002/07/owl#">
  <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
  <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">  
  <!ENTITY sourcecode "http://hut.edu.vn/ontology/sourcecode#" >
  <!ENTITY document "http://hut.edu.vn/ontology/document#" >
  <!ENTITY ruleml  "http://www.w3.org/2003/11/ruleml#" >
]>
<rdf:RDF xml:base="http://www.hut.edu.vn/ontology/rule7#" 
		xmlns:owl="http://www.w3.org/2002/07/owl#" 
		xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" 
		xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#" 
		xmlns:swrl="http://www.w3.org/2003/11/swrl#" 
		xmlns:ruleml="http://www.w3.org/2003/11/ruleml#">

	
	

	<swrl:Variable rdf:about="#class"/>
	<swrl:Variable rdf:about="#interface"/>
	<swrl:Variable rdf:about="#documentElement"/>


	<!-- Neu interface co lien quan toi docElement, Class implement no cung co co lien quan toi docElement-->
	<!-- Voi Document khong can, vi da co rule 4-->
	<swrl:Imp rdf:about="#interfaceClassDocInfer">
		<swrl:head rdf:parseType="Collection">
			<swrl:IndividualPropertyAtom>
				<swrl:propertyPredicate rdf:resource="http://hut.edu.vn/ontology/document#codeRelatedToDoc"/>
				<swrl:argument1 rdf:resource="#class"/>
				<swrl:argument2 rdf:resource="#documentElement"/>
			</swrl:IndividualPropertyAtom>
		</swrl:head>

		<swrl:body rdf:parseType="Collection">
			<swrl:ClassAtom>
				<swrl:classPredicate rdf:resource="http://hut.edu.vn/ontology/sourcecode#Interface"/>
				<swrl:argument1 rdf:resource="#interface"/>
			</swrl:ClassAtom>
			<swrl:ClassAtom>
				<swrl:classPredicate rdf:resource="http://hut.edu.vn/ontology/document#DocumentElement"/>
				<swrl:argument1 rdf:resource="#documentElement"/>
			</swrl:ClassAtom>
			<swrl:ClassAtom>
				<swrl:classPredicate rdf:resource="http://hut.edu.vn/ontology/sourcecode#Class"/>
				<swrl:argument1 rdf:resource="#class"/>
			</swrl:ClassAtom>

			<swrl:IndividualPropertyAtom>
				<swrl:propertyPredicate rdf:resource="http://hut.edu.vn/ontology/sourcecode#implementsInterface"/>
				<swrl:argument1 rdf:resource="#class"/>
				<swrl:argument2 rdf:resource="#interface"/>
			</swrl:IndividualPropertyAtom>

			<swrl:IndividualPropertyAtom>
				<swrl:propertyPredicate rdf:resource="http://hut.edu.vn/ontology/document#codeRelatedToDoc"/>
				<swrl:argument1 rdf:resource="#interface"/>
				<swrl:argument2 rdf:resource="#documentElement"/>
			</swrl:IndividualPropertyAtom>
		</swrl:body>
	</swrl:Imp>

</rdf:RDF>